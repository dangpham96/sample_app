<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to t("header-page.title-name"), "#", id: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to t("title-home"), root_path %></li>
        <li><%= link_to t("title-help"), help_path %></li>
          <% if logged_in? %>
            <li><%= link_to t("users-page.tittle-user-5"), users_path %></li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <%= t("users-page.tittle-user-6") %><b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to t("users-page.tittle-user-4"), current_user %></li>
                <li><%= link_to t("users-page.tittle-user-3"), edit_user_path(current_user) %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to t("users-page.tittle-user-2"), logout_path, method: :delete %>
                </li>
              </ul>
            </li>
          <% else %>
            <li><%= link_to t("header-page.tittle-login"), login_path %></li>
          <% end %>
      </ul>
    </nav>
  </div>
</header>
