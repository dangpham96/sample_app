<li>
  <%= gravatar_for user, size: Settings.index_user.gravatar_img %>
  <%= link_to user.name, user %>
  <% if current_user.admin? && !current_user?(user) %>
    <%= t("login-index-user.tittle-index-user2") %>
    <%= link_to "delete", user, method: :delete,
      data: { confirm: t("login-index-user.tittle-index-user3") } %>
  <% end %>
</li>
